{
  "device": {
    "id": "RTMP-Source-01",
    "name": "Remote RTMP Stream",
    "kind": "rtmp"
  },
  "capture": {
    "source_type": "rtmp",
    "source_uri": "rtmp://192.168.1.100/live/stream",
    "frame_interval_ms": 100
  },
  "detection": {
    "model_name": "yolov8n",
    "precision": "int8",
    "model_format": "onnx",
    "max_model_size_mb": 20.0,
    "max_latency_ms": 250.0
  },
  "uplink": {
    "enable": true,
    "registry_path": "postgresql://snowowl_dev:SnowOwl_Dev!@localhost/snowowl_dev",
    "device_name": "Remote RTMP Stream",
    "set_primary": false,
    "auto_detect_cameras": true
  },
  "forward": {
    "enable": true,
    "host": "",
    "port": 7500,
    "frame_interval_ms": 100,
    "reconnect_delay_ms": 2000 
  }
}